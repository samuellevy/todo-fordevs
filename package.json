{
  "name": "todo-fordevs",
  "homepage": "http://localhost:5000/",
  "version": "1.0.0",
  "description": "A simple todo app for devs.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "server": "env-cmd react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "go": "react-scripts build && concurrently --kill-others \"serve -s build\" \"electron .\"",
    "go-watch": "concurrently --kill-others \"PORT=5000 react-scripts start\" \"electron .\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samuellevy/todo-fordevs.git"
  },
  "author": "Samuel Levy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/samuellevy/todo-fordevs/issues"
  },
  "devDependencies": {
    "axios": "^0.18.0",
    "concurrently": "^4.1.0",
    "electron": "^5.0.0",
    "electron-reload": "^1.4.0",
    "serve": "^11.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "env-cmd": "^9.0.1",
    "i": "^0.3.6",
    "node-sass": "^4.12.0",
    "npm": "^6.9.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.0",
    "react-swipe-to-delete-component": "^0.5.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 firefox version",
      "last 1 chrome version",
      "last 1 safari version"
    ]
  }
}
