  "bugs": {
    "url": "https://github.com/samuellevy/todo-fordevs/issues"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "electron": "^5.0.0",
    "electron-reload": "^1.4.0",
    "serve": "^11.0.0"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
